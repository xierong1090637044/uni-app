<view class="uni-tab-bar"><scroll-view class="uni-swiper-tab" id="tab-bar" scroll-x><block wx:for="{{tabBars}}" wx:for-item="tab" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['tapTab',['$0'],[[['tabBars','',index,'type']]]]]]]}}" class="{{['swiper-tab-list',seleted_tab==tab.type?'active':'']}}" bindtap="__e">{{tab.name}}</view></block></scroll-view><scroll-view class="content" scroll-y><block wx:for="{{details}}" wx:for-item="detail" wx:for-index="index" wx:key="index"><view class="detail_item"><view class="detail_name">{{"产品:"+detail.goodsName}}</view><block wx:if="{{seleted_tab===1}}"><view class="display_flex_bet"><view><text>入库数量：</text><text style="margin-left:10rpx;color:#f30;font-weight:bold;">{{detail.num}}</text></view><view><text>入库单价：</text><text style="margin-left:10rpx;color:#f30;font-weight:bold;">{{detail.retailPrice}}</text></view></view></block><block wx:if="{{seleted_tab===-1}}"><view class="display_flex_bet"><view><text>出库数量：</text><text style="margin-left:10rpx;color:#f30;font-weight:bold;">{{detail.num}}</text></view><view><text>出库单价：</text><text style="margin-left:10rpx;color:#f30;font-weight:bold;">{{detail.retailPrice}}</text></view></view></block><block wx:if="{{seleted_tab===2}}"><view class="display_flex_bet"><view><text>退货数量：</text><text style="margin-left:10rpx;color:#f30;font-weight:bold;">{{detail.num}}</text></view><view><text>退货单价：</text><text style="margin-left:10rpx;color:#f30;font-weight:bold;">{{detail.retailPrice}}</text></view></view></block><block wx:if="{{seleted_tab===3}}"><view class="display_flex_bet"><view><text>盘点前库存：</text><text style="margin-left:10rpx;color:#f30;font-weight:bold;">{{detail.reserve}}</text></view><view><text>盘点后库存：</text><text style="margin-left:10rpx;color:#f30;font-weight:bold;">{{detail.now_reserve}}</text></view></view></block><view class="content_time">{{detail.createdAt}}</view></view></block></scroll-view></view>