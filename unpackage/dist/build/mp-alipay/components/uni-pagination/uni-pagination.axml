<view class="uni-pagination"><view class="uni-pagination__btns"><view class="{{(('uni-pagination__btn')+' '+[(currentIndex===1)?'uni-pagination--disabled':''])}}" hover-class="{{currentIndex===1?'':'uni-pagination--hover'}}" hover-start-time="{{20}}" hover-stay-time="{{70}}" data-event-opts="{{[['tap',[['clickLeft',['$event']]]]]}}" onTap="__e"><block a:if="{{showIcon}}"><uni-icon vue-id="9ce3b210-1" color="#000" size="20" type="arrowleft" onVueInit="__l"></uni-icon></block><block a:else>{{''+prevText+''}}</block></view><view class="{{(('uni-pagination__btn')+' '+[(currentIndex===maxPage)?'uni-pagination--disabled':''])}}" hover-class="{{currentIndex===maxPage?'':'uni-pagination--hover'}}" hover-start-time="{{20}}" hover-stay-time="{{70}}" data-event-opts="{{[['tap',[['clickRight',['$event']]]]]}}" onTap="__e"><block a:if="{{showIcon}}"><uni-icon vue-id="9ce3b210-2" color="#000" size="20" type="arrowright" onVueInit="__l"></uni-icon></block><block a:else>{{''+nextText+''}}</block></view></view><view class="uni-pagination__num"><input class="uni-pagination__num-current" style="text-align:center;margin-top:10rpx;" type="number" data-event-opts="{{[['blur',[['inputNumber',['$event']]]],['confirm',[['inputNumber',['$event']]]]]}}" value="{{currentIndex}}" onBlur="__e" onConfirm="__e"/></view></view>