<view data-event-opts="{{[['tap',[['onClick',['$event']]]]]}}" class="{{(('uni-card')+' '+(isFull?'uni-card--full':''))}}" onTap="__e"><block a:if="{{title}}"><view class="uni-card__header"><block a:if="{{thumbnail}}"><view class="uni-card__header-extra-img-view"><image class="uni-card__header-extra-img" src="{{thumbnail}}"></image></view></block><view class="uni-card__header-title-text">{{title}}</view><block a:if="{{extra}}"><view class="uni-card__header-extra-text">{{extra}}</view></block></view></block><view class="uni-card__content uni-card__content--pd"><slot></slot></view><block a:if="{{note}}"><view class="uni-card__footer">{{note}}</view></block></view>