<view class="z-table"><view class="z-table-main" style="{{(compluteHeight)}}"><block a:if="{{!tableLoaded&&(!tableData||!columns)}}"><view class="{{(('z-loading')+' '+[(tableShow)?'ztableLoading':''])}}"><view class="z-loading-animate"></view></view></block><view class="z-table-container"><view class="z-table-pack"><view class="z-table-title"><block a:for="{{$root.l0}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['sort',['$0',index],[[['columns','',index,'key']]]]]]]}}" class="{{(('z-table-title-item')+' '+((stickSide&&index==0)?'z-table-stick-side':''))}}" style="{{'width:'+(item.$orig.width?item.$orig.width+'rpx':'200rpx')+';'}}" onTap="__e"><block a:if="{{showSelect&&!singleSelect&&index===0}}"><view data-event-opts="{{[['tap',[['doSelect',[true]]]]]}}" class="select-box" onTap="__e"><view class="{{(('select-tip')+' '+[(selectAll)?'selected':''])}}"></view></view></block><view class="{{(('z-table-col-text')+' '+[(titleTextAlign==='left')?'text-left':'',(titleTextAlign==='center')?'text-center':'',(titleTextAlign==='right')?'text-right':''])}}"><view><rich-text nodes="{{item.m0}}"></rich-text></view><block a:if="{{item.g0&&item.g1&&tableData.length}}"><view class="sort"><view class="{{(('up-arrow')+' '+((nowSortKey==item.$orig.key&&sortType=='asc')?'action':''))}}"></view><view class="{{(('down-arrow')+' '+((nowSortKey==item.$orig.key&&sortType=='desc')?'action':''))}}"></view></view></block></view></view></block></view><block a:if="{{tableData.length}}"><view class="{{(('table-container-box')+' '+[(!longTable&&showBottomSum)?'short-table':''])}}"><block a:for="{{$root.l2}}" a:for-item="row" a:for-index="iIndex" a:key="iIndex"><view class="{{(('z-table-container-row')+' '+((showBottomSum)?'z-table-has-bottom':''))}}"><block a:for="{{row.l1}}" a:for-item="col" a:for-index="jIndex" a:key="jIndex"><view data-event-opts="{{[['tap',[['itemClick',['$0','$1'],[[['tableData','',iIndex]],[['columns','',jIndex]]]]]]]}}" class="{{(('z-table-container-col')+' '+[(stickSide&&jIndex==0)?'z-table-stick-side':''])}}" style="{{'width:'+(col.$orig.width?col.$orig.width+'rpx':'200rpx')+';'}}" onTap="__e"><block a:if="{{showSelect&&jIndex===0}}"><view data-event-opts="{{[['tap',[['doSelect',[false,iIndex]]]]]}}" class="select-box" onTap="__e"><view class="{{(('select-tip')+' '+[(row.g2)?'selected':''])}}"></view></view></block><view class="{{(('z-table-col-text')+' '+[(textAlign==='left')?'text-left':'',(textAlign==='center')?'text-center':'',(textAlign==='right')?'text-right':''])}}"><block a:if="{{!col.$orig.isLink}}"><view><rich-text nodes="{{col.m1}}"></rich-text></view></block><block a:else><block a:if="{{col.$orig.isLink}}"><navigator url="{{col.m2}}"><rich-text nodes="{{col.m3}}"></rich-text></navigator></block></block></view></view></block></view></block></view></block><block a:if="{{showBottomSum&&tableData.length}}"><view class="{{(('z-table-bottom')+' '+[(longTable)?'long-table':''])}}"><block a:for="{{$root.l3}}" a:for-item="sumCol" a:for-index="sumIndex" a:key="sumIndex"><view class="{{(('z-table-bottom-col')+' '+((stickSide&&sumIndex==0)?'z-table-stick-side':''))}}" style="{{'width:'+(sumCol.$orig.width?sumCol.$orig.width+'rpx':'200rpx')+';'}}"><view class="z-table-bottom-text"><text class="{{((sumIndex==0)?'sum':'')}}">{{sumIndex==0?'总计':sumCol.m4}}</text></view></view></block></view></block></view></view><block a:if="{{tableData&&tableData.length==0&&!tableLoaded}}"><view class="table-empty"><view><rich-text nodes="{{showLoading?'':emptyText}}"></rich-text></view></view></block></view></view>