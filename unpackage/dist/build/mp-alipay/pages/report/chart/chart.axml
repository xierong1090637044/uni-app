<view class="qiun-columns"><view class="display_flex_bet"><view style="text-align:center;width:50%;"><picker range="{{chartTypes}}" data-event-opts="{{[['change',[['select_type',['$event']]]]]}}" onChange="__e"><view class="uni-input display_flex" style="justify-content:center;"><text style="margin-right:20rpx;">{{chartTypes[chartValue]}}</text><fa-icon vue-id="40243e1e-1" type="angle-down" size="20" color="#3d3d3d" onVueInit="__l"></fa-icon></view></picker></view><view style="text-align:center;width:50%;"><picker mode="date" value="{{now_day}}" fields="month" data-event-opts="{{[['change',[['bindDateChange',['$event']]]]]}}" onChange="__e"><view class="uni-input display_flex" style="justify-content:center;"><text style="margin-right:20rpx;">{{now_day}}</text><fa-icon vue-id="40243e1e-2" type="angle-down" size="20" color="#3d3d3d" onVueInit="__l"></fa-icon></view></picker></view></view><block a:if="{{chartValue==0}}"><dchart vue-id="40243e1e-3" now_day="{{now_day}}" onVueInit="__l"></dchart></block><block a:if="{{chartValue==1}}"><achart vue-id="40243e1e-4" now_day="{{now_day}}" onVueInit="__l"></achart></block><block a:if="{{chartValue==2||chartValue==3}}"><bchart vue-id="40243e1e-5" now_day="{{now_day}}" type="{{chartValue}}" onVueInit="__l"></bchart></block><block a:if="{{chartValue==4}}"><cchart vue-id="40243e1e-6" onVueInit="__l"></cchart></block></view>