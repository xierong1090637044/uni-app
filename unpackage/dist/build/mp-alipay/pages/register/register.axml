<view><view style="display:flex;justify-content:space-between;padding:15px;"><view style="font-size:18px;"></view><view style="font-size:12px;"></view></view><view class="header_text"><view>注册</view><view class="iconImage"><image style="width:100%;height:100%;" src="/static/chuan.png"></image></view></view><view style="padding:15px 30px;margin:5% 0;"><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" onSubmit="__e"><view class="input_view"><input class="input_element" placeholder="请输入手机号" name="phone" maxlength="11" data-event-opts="{{[['input',[['get_InputPhone',['$event']]]]]}}" onInput="__e"/></view><view class="input_view_flex"><view style="width:60%;"><input class="input_element" placeholder="请输入验证码" maxlength="6" name="sms_code"/></view><view><button class="get_smscode" plain="true" disabled="{{code_button_state}}" data-event-opts="{{[['tap',[['get_smscode',['$event']]]]]}}" onTap="__e">{{code_text}}</button></view></view><view><button class="login_button" plain="true" form-type="submit" hover-class="bg_button">注册</button></view><button class="display_flex wechat_login" open-type="getAuthorize" plain="true" scope="phoneNumber" data-event-opts="{{[['getAuthorize',[['$onAliGetAuthorize',['get_userInfo','$event']]]],['error',[['$onAliAuthError',['get_userInfo','$event']]]]]}}" onGetAuthorize="__e" onError="__e"><fa-icon style="margin-right:20rpx;" vue-id="9a7b6094-1" type="wechat" size="18" color="#26cf23" onVueInit="__l"></fa-icon><view style="font-size:24rpx;">微信注册</view></button><view class="footer"><text data-event-opts="{{[['tap',[['isShowAgree',['$event']]]]]}}" class="{{(showAgree?'cuIcon-radiobox':'cuIcon-round')}}" style="margin-right:10rpx;" onTap="__e"></text><text>同意</text><navigator style="font-size:24rpx;" url="user_protocol/user_protocol" open-type="navigate">《用户协议》</navigator></view></form></view></view>