<view class="container"><view class="page-body"><scroll-view class="nav-left" style="{{('height:'+height+'px')}}" scroll-y><block a:for="{{custom_list}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['categoryClickMain',['$0',index],[[['custom_list','',index,'objectId']]]]]]]}}" class="{{(('nav-left-item')+' '+(index==categoryActive?'active':''))}}" onTap="__e">{{''+item.custom_name+''}}</view></block></scroll-view><scroll-view class="nav-right" style="{{('height:'+height+'px')}}" scroll-y scroll-top="{{scrollTop}}" scroll-with-animation data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" onScroll="__e"><block a:for="{{subCategoryList}}" a:for-item="c" a:for-index="index" a:key="index"><view class="nav-right-item" id="{{index===0?'first':''}}"><view style="text-align:left;border-bottom:solid 1rpx #E0E0E0;padding:20rpx 0;"><view>{{"销售商品："+c.goodsName}}</view><view class="display_flex_bet"><view>{{"实际单价："+c.goodsId.retailPrice}}</view><view>{{"卖出单价："+c.retailPrice}}</view></view><view class="display_flex_bet"><view>{{"数量："+c.num}}</view></view><block a:if="{{c.operater}}"><view>{{"操作者："+c.operater.nickName}}</view></block><block a:else><block a:if="{{c.opreater}}"><view>{{"操作者："+c.opreater.nickName}}</view></block></block><view>{{c.createdAt}}</view></view></view></block></scroll-view></view></view>