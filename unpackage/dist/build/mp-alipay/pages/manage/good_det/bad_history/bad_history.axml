<view><block a:if="{{detail!=''}}"><view class="all_list"><block a:for="{{detail}}" a:for-item="item" a:for-index="index" a:key="index"><view style="display:flex;justify-content:space-between;border-bottom:1px solid#ddd;padding:5px 0;"><view><view>{{item.createdAt}}</view><view>{{"货损数量："+item.bad_num}}</view><block a:if="{{item.beizhu_text!=''}}"><view>{{"备注："+item.beizhu_text}}</view></block></view></view></block></view></block><block a:else><view class="no_operations">暂无货损记录</view></block></view>