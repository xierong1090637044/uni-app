<scroll-view class="page" scroll-y="true"><block a:for="{{model}}" a:for-item="item" a:for-index="key" a:key="key"><view class="frist" style="margin-bottom:30rpx;"><view class="input_item"><view class="left_item">颜色</view><view class="right_input1 display_flex_bet" style="width:calc(100% - 200rpx);"><input placeholder="请输入颜色名" data-event-opts="{{[['input',[['__set_model',['$0','value','$event',[]],[[['model','',key,'custom1']]]]]]]}}" value="{{item.custom1.value}}" onInput="__e"/><block a:if="{{key>=1}}"><fa-icon vue-id="{{'72db103c-1-'+key}}" type="minus-square-o" size="20" color="#2ca879" data-event-opts="{{[['^click',[['reduce_this',[key]]]]]}}" onClick="__e" onVueInit="__l"></fa-icon></block></view></view><view class="input_item"><view class="left_item">尺寸</view><view class="right_input1"><input placeholder="尺寸" data-event-opts="{{[['input',[['__set_model',['$0','value','$event',[]],[[['model','',key,'custom2']]]]]]]}}" value="{{item.custom2.value}}" onInput="__e"/></view></view><view class="input_item"><view class="left_item"><input placeholder="自定义规格名1" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],[[['model','',key,'custom3']]]]]]]}}" value="{{item.custom3.name}}" onInput="__e"/></view><view class="right_input1"><input placeholder="请输入自定义规格1的值" data-event-opts="{{[['input',[['__set_model',['$0','value','$event',[]],[[['model','',key,'custom3']]]]]]]}}" value="{{item.custom3.value}}" onInput="__e"/></view></view><view class="input_item"><view class="left_item"><input placeholder="自定义规格名2" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],[[['model','',key,'custom4']]]]]]]}}" value="{{item.custom4.name}}" onInput="__e"/></view><view class="right_input1"><input placeholder="请输入自定义规格2的值" data-event-opts="{{[['input',[['__set_model',['$0','value','$event',[]],[[['model','',key,'custom4']]]]]]]}}" value="{{item.custom4.value}}" onInput="__e"/></view></view><block a:if="{{index}}"><view class="input_item"><view class="left_item">库存</view><view class="right_input1"><input placeholder="请输入库存数量" type="number" data-event-opts="{{[['input',[['__set_model',['$0','reserve','$event',[]],[[['model','',key]]]]]]]}}" value="{{item.reserve}}" onInput="__e"/></view></view></block></view></block><block a:if="{{index==''||index==null}}"><view class="input_item2 frist1" style="margin:30rpx 0;justify-content:center;" hover-class="none" url="/pages/manage/warehouse/warehouse?type=choose_more" data-event-opts="{{[['tap',[['push_model',['$event']]]]]}}" onTap="__e"><view style="display:flex;align-items:center;"><view class="left_item" style="color:#2ca879;">增加规格</view><fa-icon vue-id="72db103c-2" type="plus" size="20" color="#2ca879" onVueInit="__l"></fa-icon></view></view></block><button data-event-opts="{{[['tap',[['confrim_this',['$event']]]]]}}" class="confrim_button" onTap="__e">确定</button></scroll-view>