<view><block a:if="{{loading}}"><loading vue-id="535acdec-1" onVueInit="__l"></loading></block><block a:else><view><view class="uni-common-mt"><uni-segmented-control vue-id="535acdec-2" current="{{current}}" values="{{items}}" style-type="button" active-color="#426ab3" data-event-opts="{{[['^clickItem',[['onClickItem']]]]}}" onClickItem="__e" onVueInit="__l"></uni-segmented-control></view><view><block a:if="{{logsList&&logsList.length>0}}"><scroll-view class="content" scroll-y="true"><block a:for="{{logsList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="list_item"><view class="display_flex"><fa-icon vue-id="{{'535acdec-3-'+index}}" type="info-circle" size="20" color="#426ab3" onVueInit="__l"></fa-icon><view class="information">{{item.log}}</view></view><view class="option_time">{{"记录于"+item.createdAt}}</view></view></block></scroll-view></block><block a:else><view class="content"><nocontent vue-id="535acdec-4" type="{{1}}" onVueInit="__l"></nocontent></view></block><view style="padding:6rpx 0;border-top:1rpx solid#ddd;"><uni-pagination vue-id="535acdec-5" show-icon="{{true}}" total="100000" current="{{page_num}}" data-event-opts="{{[['^change',[['change_page',['$event']]]]]}}" onChange="__e" onVueInit="__l"></uni-pagination></view></view></view></block></view>