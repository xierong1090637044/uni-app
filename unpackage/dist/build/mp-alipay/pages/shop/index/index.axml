<view><view class="header" style="{{'position:'+(headerPosition)+';'+('top:'+(headerTop)+';')}}"><block a:for="{{orderbyList}}" a:for-item="target" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['select',[index]]]]]}}" class="{{(('target')+' '+(target.selected?'on':''))}}" onTap="__e">{{''+target.text+''}}<block a:if="{{target.orderbyicon}}"><view class="{{(('icon')+' '+target.orderbyicon[target.orderby])}}"></view></block></view></block></view><view class="place"></view><view class="goods-list" style="margin-top:20rpx;"><view class="product-list"><block a:for="{{goodsList}}" a:for-item="goods" a:for-index="__i0__" a:key="objectId"><view data-event-opts="{{[['tap',[['toGoods',['$0'],[[['goodsList','objectId',goods.objectId]]]]]]]}}" class="product" onTap="__e"><image style="width:100%;height:200rpx;" mode="aspectFit" src="{{goods.goodsIcon}}"></image><view class="name">{{goods.goodsName}}</view><view class="info"><view class="price">{{"￥"+goods.retailPrice}}</view><block a:if="{{userId!='b9rmzv49'}}"><view class="slogan">{{"库存"+goods.reserve+(goods.packingUnit?goods.packingUnit:'')}}</view></block></view></view></block></view><view class="loading-text">{{loadingText}}</view></view></view>