<view><view class="footer"><view class="icons"><view data-event-opts="{{[['tap',[['share',['$event']]]]]}}" class="box" onTap="__e"><fa-icon vue-id="2f950782-1" type="share" size="20" color="#999" onVueInit="__l"></fa-icon><view class="text">分享</view></view></view><view class="btn"><view data-event-opts="{{[['tap',[['buy',['$event']]]]]}}" class="buy" onTap="__e">立即购买</view></view></view><view data-event-opts="{{[['touchMove',[['discard',['$event']]]],['tap',[['hideSpec',['$event']]]]]}}" class="{{(('popup spec')+' '+specClass)}}" catchTouchMove="__e" onTap="__e"><view class="mask"></view><view data-event-opts="{{[['tap',[['discard',['$event']]]]]}}" class="layer" catchTap="__e"><view class="content"><view class="length"><view class="text">数量</view><view class="number"><view data-event-opts="{{[['tap',[['sub',['$event']]]]]}}" class="sub" catchTap="__e"><fa-icon style="line-height:60rpx;" vue-id="2f950782-2" type="minus" size="20" color="#999" onVueInit="__l"></fa-icon></view><view data-event-opts="{{[['tap',[['discard',['$event']]]]]}}" class="input" catchTap="__e"><input type="digit" data-event-opts="{{[['blur',[['wetherNum']]],['input',[['__set_model',['$0','number','$event',[]],['goodsData']]]]]}}" value="{{goodsData.number}}" onBlur="__e" onInput="__e"/></view><view data-event-opts="{{[['tap',[['add',['$event']]]]]}}" class="add" catchTap="__e"><fa-icon style="line-height:60rpx;" vue-id="2f950782-3" type="plus" size="20" color="#999" onVueInit="__l"></fa-icon></view></view></view></view><view class="btn"><view data-event-opts="{{[['tap',[['createOrder',['$event']]]]]}}" class="button" onTap="__e">完成</view></view></view></view><view class="swiper-box"><swiper circular="true" autoplay="true" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" onChange="__e"><block a:for="{{swiperList}}" a:for-item="swiper" a:for-index="__i0__" a:key="id"><swiper-item><image src="{{swiper.img}}"></image></swiper-item></block></swiper><view class="indicator">{{currentS